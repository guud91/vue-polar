{
  "name": "vue-polar",
  "version": "0.4.2",
  "description": "Vue component to easily create polar layouts (organize in circle/spiral)",
  "author": {
    "name": "Roy Prins",
    "email": "prinsroy@gmail.com"
  },
  "repository": "https://github.com/snirp/vue-polar",
  "bugs": "https://github.com/snirp/vue-polar/issues",
  "keywords": [
    "vue",
    "circle",
    "layout",
    "polar",
    "functional",
    "component"
  ],
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "dist/vuePolar.umd.js",
  "unpkg": "dist/vuePolar.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "package": "vue-cli-service build --target lib --name vuePolar src/components/Polar.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
